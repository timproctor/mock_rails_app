<div>
  <article id="user">
    <h1>
      <%= user_grammer %>
    </h1>
    <div>
      <% @users.each do |user| %>

        <div>
          <h2>
            <%= user.name %>
          </h2>
          <h3>
            <%= user.email%>
          </h3>
        </div>
        <div>
          <p>
            <%= link_to "Show #{user.name}", user_path(user)%>
          </p>
        </div>
      <% end %>
    </div>
  </article>
</div>
